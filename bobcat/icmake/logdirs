list logDirs(string source, string dest)
{
    list dirs;
    list ret;
    string entry;
    int idx;
    int nDirs;

    dirs = backtick("(chdir " + source + ";find ./ -type d)");
    nDirs = sizeof(dirs);

    echo(OFF);
    for (idx = 0; idx != nDirs; ++idx)
    {
        ret += (list)cutEoln(dirs[idx]);
        md(dest + "/" + ret[idx]);
    }
    echo(g_echo);

    return ret;
}
